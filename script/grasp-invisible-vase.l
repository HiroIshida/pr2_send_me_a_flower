(load "package://pr2eus/pr2-interface.l")
(load "util.l")
(pr2-init)
(setq *robot* *pr2*)
(setq *r* *robot*)
(setq w 50)
(dotimes (i 5)
  (send *robot* :rarm :move-end-pos (float-vector (* w 1) 20 0) :world)
  (transmit-posture t)
  (send *robot* :rarm :move-end-pos (float-vector (* w -1) 20 0) :world)
  (transmit-posture t)
  )

(send *robot* :rarm :move-end-pos (float-vector 0 0 100) :world)
(transmit-posture t)
